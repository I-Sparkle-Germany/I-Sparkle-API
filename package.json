{
  "name": "wise",
  "version": "5.0.0",
  "description": "Web-based Inquiry Science Environment",
  "main": "app.js",
  "dependencies": {
    "compass": "^0.1.1",
    "eslint": "^1.7.3",
    "jspm": "^0.16.19",
    "sass": "^0.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "browser-sync": "^2.11.1",
    "connect-history-api-fallback": "^1.1.0",
    "cssnano": "^3.4.0",
    "gulp": "^3.9.0",
    "gulp-if": "^2.0.0",
    "gulp-newer": "^1.1.0",
    "gulp-postcss": "^6.0.1",
    "gulp-print": "^2.0.1",
    "gulp-sass": "^2.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.7",
    "karma-jspm": "^2.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.3"
  },
  "scripts": {
    "browser-sync": "./node_modules/browser-sync/bin/browser-sync.js start --config bs-config.js",
    "postinstall": "cd src/main/webapp/wise5; npm install jspm; jspm install -y",
    "watch-all": "npm run transpile & npm run watch-sass",
    "transpile": "./node_modules/babel-cli/bin/babel.js --extensions .es6 --source-maps --presets babel-preset-es2015 --watch src/main/webapp/wise5/ --only wise5/services,wise5/classroomMonitor,wise5/authoringTool,wise5/directives,wise5/components,wise5/vle,wise5/controllers,wise5/node/,wise5/test-unit --out-dir src/main/webapp/wise5/",
    "watch-sass": "gulp",
    "compile-sass": "gulp compile-sass",
    "test": "./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "test-watch": "./node_modules/gulp-helpers/node_modules/karma/bin/karma start --single-run --browsers Firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WISE-Community/WISE.git"
  },
  "keywords": [
    "WISE",
    "education",
    "science",
    "inquiry"
  ],
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/WISE-Community/WISE/issues"
  },
  "homepage": "https://github.com/WISE-Community/WISE#readme",
  "jspm": {
    "directories": {
      "baseURL": "src/main/webapp/wise5",
      "packages": "jspm_packages"
    },
    "dependencies": {
      "jquery": "github:components/jquery@^2.1.4",
      "angular": "github:angular/bower-angular@^1.5.0",
      "angular-mocks": "github:angular/bower-angular-mocks@^1.5.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
